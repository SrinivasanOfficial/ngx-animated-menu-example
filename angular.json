{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "AppMod": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "root": "projects/app-mod",
      "sourceRoot": "projects/app-mod/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-mod",
            "index": "projects/app-mod/src/index.html",
            "browser": "projects/app-mod/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/app-mod/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-mod/src/favicon.ico",
              "projects/app-mod/src/assets"
            ],
            "styles": [
              "projects/app-mod/src/styles.scss",
              "projects/app-mod/src/icons.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "AppMod:build:production"
            },
            "development": {
              "buildTarget": "AppMod:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "AppMod:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/app-mod/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-mod/src/favicon.ico",
              "projects/app-mod/src/assets"
            ],
            "styles": ["projects/app-mod/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "AppStand": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app-stand",
      "sourceRoot": "projects/app-stand/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app-stand",
            "index": "projects/app-stand/src/index.html",
            "browser": "projects/app-stand/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/app-stand/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-stand/src/favicon.ico",
              "projects/app-stand/src/assets"
            ],
            "styles": ["projects/app-stand/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "AppStand:build:production"
            },
            "development": {
              "buildTarget": "AppStand:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "AppStand:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/app-stand/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/app-stand/src/favicon.ico",
              "projects/app-stand/src/assets"
            ],
            "styles": ["projects/app-stand/src/styles.scss"],
            "scripts": []
          }
        }
      }
    },
    "ngx-animated-menu": {
      "projectType": "library",
      "root": "projects/ngx-animated-menu",
      "sourceRoot": "projects/ngx-animated-menu/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-animated-menu/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-animated-menu/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-animated-menu/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-animated-menu/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "ngx-animated-circular-menu": {
      "projectType": "library",
      "root": "projects/ngx-animated-circular-menu",
      "sourceRoot": "projects/ngx-animated-circular-menu/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-animated-circular-menu/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-animated-circular-menu/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-animated-circular-menu/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-animated-circular-menu/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    }
  }
}
